		part { name: "event_box";
			type: SWALLOW;
			scale: 1;
			mouse_events: 1;
			repeat_events: 1;
			description { state: "default" 0.0;
				rel1.to: "albumart";
				rel2.to: "albumart";
			}
		}

		part { name: "bg_edit";
			type: IMAGE;
			scale: 1;
			mouse_events: 1;
			repeat_events: 1;
			description { state: "default" 0.0;
				visible: 0;
			}
			description { state: "show" 0.0;
				rel1 { relative: 0 0; }
				rel2 { relative: 1 1;}
				image { normal: IMG_MW_FRAME_EDIT;
					border: 176 176 67 106;
				}
				visible: 1;
			}
		}

		part { name: "size_bt";
