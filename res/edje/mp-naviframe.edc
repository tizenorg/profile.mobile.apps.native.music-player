#define MP_NAVIFRAME_TITLE_STYLE_COLOR_INC "#3B73B6"
#define MP_NAVIFRAME_SUBTITLE_TEXT_COLOR_INC 128 128 128 255
#define MP_NAVIFRAME_SUBTITLE_FONT_SIZE_INC 32
