		}

		part { name: "progress_bar_confine";
			type: RECT;
			scale: 1;
			mouse_events: 0;
			description { state: "default" 0.0;
				rel1 { relative: 0.5 0.0; to: "progress_bg"; offset: 0 (EQ_HANDLER_H/-2); }
